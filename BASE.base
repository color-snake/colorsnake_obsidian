views:
  - type: cards
    name: PALETTES
    filters:
      and:
        - file.folder.startsWith("Color Snake Palettes")
    sort:
      - property: img
        direction: ASC
    image: note.img
  - type: table
    name: Table
  - type: cards
    name: COLORS
    filters:
      and:
        - file.inFolder("Color Names")
    image: note.img
    imageAspectRatio: 0.5
