views:
  - type: table
    name: Table
  - type: cards
    name: COLORS
    filters:
      and:
        - file.inFolder("Color Names")
    image: note.img
    imageAspectRatio: 0.5
  - type: cards
    name: PALETTES
    filters:
      and:
        - file.folder == "Color Snake Palettes"
    image: note.img
